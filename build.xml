<?xml version="1.0"?>

<project name="DfObjectFactory-1.0" basedir="." default="release" xmlns:ht="happytrails">

  <property name="ht.include.cfg" value="true" />
  <property name="allow.empty.sources.dir" value="true" />

  <import file="${happytrails.root}/happytrails.xml"/>

  <ht:import file="happier-trails.xml" optional="false"/>
  <ht:import file="copy-with-permissions.xml" optional="false" />

  <property name="findbugs.failOnError" value="true" />
  <property name="findbugs.report.level" value="high" />
  <property name="ht.findbugs.always.report" value="true" />

  <property name="checkstyle.configuration.file" location="${basedir}/checkstyle-config.xml"/>
       
</project>
